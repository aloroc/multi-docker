{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
        "name": "client",
        "image": "aloroc/multi-client",
        "hostname": "client",
        "essential": false
        },
        
        {
        "name": "server",
        "image": "aloroc/multi-server",
        "hostname": "api",
        "essential": false
        },
        
        {
        "name": "worker",
        "image": "aloroc/multi-worker",
        "hostname": "worker",
        "essential": false
        },
        
        {
        "name": "nginx",
        "image": "aloroc/multi-nginx",
        "essential": true,
        "portMappings": [
            {
            "hostPort": 80,
            "containerPort": 80
        }
        ],
        "links": ["client", "server"]
        }
    ]
}
